{
  "extends": ["prettier"],
  "env": {
    "browser": true
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "all"
      }
    ],
    "no-unused-vars": 1,
    "no-console": 1
  },
  "globals": {
    "$": false
  }
}